{
	"name": "generate-component-module",
	"displayName": "Module and Component Generator",
	"description": "Generates a module and component in vue",
	"version": "0.0.1",
	"publisher": "janic0",
	"repository": "https://github.com/janic0/module-component-generator",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.createModule",
		"onCommand:extension.createComponent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createModule",
				"title": "Create Module"
			},
			{
				"command": "extension.createComponent",
				"title": "Create Component"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.5.5"
	}
}
